{
  "name": "fennec-ui",
  "version": "1.0.7",
  "description": "UI package for Fennec Toolkit",
  "author": "tarasovin",
  "license": "MIT",
  "repository": "tarasov-in/fennec-ui",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle-crl --jsx React.createElement --jsxFragment React.Fragment --no-compress --format modern,cjs",
    "start": "microbundle-crl watch --jsx React.createElement --jsxFragment React.Fragment --no-compress --format modern,cjs",
    "prepare": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && npm install && npm run build",
    "deploy": "gh-pages -d example/build"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "pubsub-js": "^1.9.4",
    "antd": "~4.20",
    "antd-mobile": "^2.3.4",
    "moment": "^2.29.3",
    "moment-timezone": "^0.5.34",
    "numeral": "^2.0.6",
    "react-uuid": "^1.0.2",
    "jwt-decode": "^3.1.2",
    "react-router-dom": "^6.3.0",
    "react-jss": "^10.9.0",
    "rc-util": "^5.22.5",
    "react-icofont": "^1.0.19",
    "react-responsive": "^9.0.0-beta.10"
  },
  "devDependencies": {
    "antd": "~4.20",
    "antd-mobile": "^2.3.4",
    "babel-eslint": "^10.0.3",
    "cross-env": "^7.0.3",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-standard": "^4.1.0",
    "gh-pages": "^4.0.0",
    "jwt-decode": "^3.1.2",
    "microbundle-crl": "^0.13.11",
    "moment": "^2.29.3",
    "moment-timezone": "^0.5.34",
    "npm-run-all": "^4.1.5",
    "numeral": "^2.0.6",
    "prettier": "^2.7.1",
    "pubsub-js": "^1.9.4",
    "rc-util": "^5.22.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icofont": "^1.0.19",
    "react-jss": "^10.9.0",
    "react-responsive": "^9.0.0-beta.10",
    "react-router-dom": "^6.3.0",
    "react-scripts": "^5.0.1",
    "react-uuid": "^1.0.2"
  },
  "files": [
    "dist"
  ]
}
