{
  "name": "fennec-ui",
  "version": "1.1.273",
  "description": "UI package for Fennec Toolkit",
  "author": "tarasovin",
  "license": "MIT",
  "repository": "tarasov-in/fennec-ui",
  "main": "dist/fennec-ui.js",
  "module": "dist/fennec-ui.modern.js",
  "source": "src/index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle-crl -o ./dist --no-css-modules --no-compress --format modern,cjs",
    "prestart": "mkdir -p ../fennec-ui-dev && cp -r ./package.json ../fennec-ui-dev/",
    "start": "microbundle-crl watch -o ../fennec-ui-dev/dist --no-css-modules --no-compress --format modern,cjs",
    "buildorigin": "microbundle-crl --no-compress --format modern,cjs",
    "startorigin": "microbundle-crl watch --no-compress --format modern,cjs",
    "prepare": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && npm install && npm run build",
    "deploy": "gh-pages -d example/build"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "pubsub-js": "^1.9.4",
    "antd": "5.6.4",
    "antd-mobile": "v5.28.1",
    "moment": "^2.29.3",
    "moment-timezone": "^0.5.34",
    "numeral": "^2.0.6",
    "react-uuid": "^1.0.2",
    "jwt-decode": "^3.1.2",
    "react-router-dom": "^6.3.0",
    "react-jss": "^10.9.0",
    "rc-util": "^5.22.5",
    "fabric": "^5.3.0",
    "react-icofont": "^1.0.19",
    "react-responsive": "^9.0.0-beta.10",
    "react-virtualized": "^9.22.3"
  },
  "devDependencies": {
    "antd": "5.6.4",
    "antd-mobile": "^5.15.1",
    "babel-eslint": "^10.0.3",
    "cross-env": "^7.0.3",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-standard": "^4.1.0",
    "gh-pages": "^4.0.0",
    "jwt-decode": "^3.1.2",
    "microbundle-crl": "^0.13.11",
    "moment": "^2.29.3",
    "moment-timezone": "^0.5.34",
    "npm-run-all": "^4.1.5",
    "numeral": "^2.0.6",
    "prettier": "^2.7.1",
    "pubsub-js": "^1.9.4",
    "rc-util": "^5.22.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icofont": "^1.0.19",
    "react-jss": "^10.9.0",
    "react-responsive": "^9.0.0-beta.10",
    "react-router-dom": "^6.3.0",
    "react-scripts": "^5.0.1",
    "react-uuid": "^1.0.2",
    "react-virtualized": "^9.22.3"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "dayjs": "^1.11.9",
    "device-uuid": "^1.0.4",
    "js-cookie": "^3.0.1",
    "less": "^4.1.3",
    "refresh-fetch": "^0.8.0"
  }
}
